import org.eclipse.emf.ecore.resource.impl.ResourceSetImpl;
import org.eclipse.emf.ecore.xmi.impl.XMIResourceFactoryImpl;

import Diff.DiffPackage;

/**
 * Hello world!
 *
 */
public class App 
{
    public static void main( String[] args )
    {    	
    	Resource.Factory.Registry.INSTANCE.getExtensionToFactoryMap().put("xmi", new XMIResourceFactoryImpl());
    	EPackage.Registry.INSTANCE.put("ExamplePackage.eINSTANCE.getNsURI()", DiffPackage.eINSTANCE);
        Resource.Factory.Registry reg = Resource.Factory.Registry.INSTANCE;
        Map<String, Object> m = reg.getExtensionToFactoryMap();
        m.put("xmi", new XMIResourceFactoryImpl());
        ResourceSet resSet = new ResourceSetImpl();
        Resource resource = resSet.getResource(URI.createURI("/Users/diegoprietotorres/Documents/programs/DynamicComparator/dynamiccomparator/models/diff.xmi"), true);        
        EObject root = (EObject)resource.getContents().get(0);        
        int i = 0;
    }
